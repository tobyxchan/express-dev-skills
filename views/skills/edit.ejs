<%- include('../partials/header') %>

<form action="/skills/<%= skill.id %>?_method=PUT" method="POST">
  <input type="text" name="skill" value="<%= skill.skill %>"><br>
  <label>
    <input type="checkbox" name="proficient" <%= skill.proficient ? 'checked' : '' %>>
    Proficient
  </label><br>
  <button type="submit">Update Skill</button>
</form>

<%- include('../partials/footer') %>